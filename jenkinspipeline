pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
          stage('Build') {
            steps {
                sh 'javac Main.java'
                echo 'hi'
            }
        }
        stage('Run') {
            steps {
                sh 'java Main'
            }
        }
    }
}
